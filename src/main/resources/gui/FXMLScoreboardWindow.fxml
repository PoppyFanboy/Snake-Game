<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Scene  xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="poppyfanboy.snakegame.gui.ScoreboardWindowController"
        fx:id = "scoreboardScene" width = "350" height = "500">
    <TabPane tabClosingPolicy = "UNAVAILABLE" fx:id = "tabPane">
        <tabs>
            <Tab text = "Empty">
                <FlowPane fx:id = "scoreboardPane1" orientation = "VERTICAL">
                </FlowPane>
            </Tab>
            <Tab text = "No Way Out">
                <FlowPane fx:id = "scoreboardPane2" orientation = "VERTICAL">
                </FlowPane>
            </Tab>
            <Tab text = "Rugby Field">
                <FlowPane fx:id = "scoreboardPane3" orientation = "VERTICAL">
                </FlowPane>
            </Tab>
            <Tab text = "Spiral">
                <FlowPane fx:id = "scoreboardPane4" orientation = "VERTICAL">
                </FlowPane>
            </Tab>
            <Tab text = "Claustrophobia">
                <FlowPane fx:id = "scoreboardPane5" orientation = "VERTICAL">
                </FlowPane>
            </Tab>
        </tabs>
    </TabPane>
</Scene>
